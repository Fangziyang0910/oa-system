<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.whaler.oasys.mapper.ApproverMapper">
    <insert id="insertApproverEntity">
        insert into approver(approver_id, process_instance_id) values (#{approverId}, #{processInstanceId})
    </insert>

    <select id="selectByApproverId">
        select * from approver where approver_id = #{approverId}
    </select>
</mapper>