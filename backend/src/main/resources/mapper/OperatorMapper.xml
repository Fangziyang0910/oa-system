<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.whaler.oasys.mapper.OperatorMapper">
    <insert id="insertOperatorEntity">
        insert into operator(operator_id, process_instance_id) 
        values (#{operatorId}, #{processInstanceId})
    </insert>
    
    <select id="selectByOperatorId">
        select * from operator where operator_id = #{operatorId}
    </select>
</mapper>