<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.whaler.oasys.mapper.ApplicantMapper">
    <insert id="insertApplicantEntity">
        insert into applicant(operator_id, process_instance_id) values (#{operatorId}, #{processInstanceId})
    </insert>

    <select id="selectByApplicantId">
        select * from applicant where operator_id = #{applicantId}
    </select>
</mapper>